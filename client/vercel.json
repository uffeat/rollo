{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },

    {
      "source": "/content/:match*",
      "destination": "/content/:match*"
    },

    {
      "source": "/data/:match*",
      "destination": "/data/:match*"
    },

    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },

    {
      "source": "/images/:match*",
      "destination": "/images/:match*"
    },

    {
      "source": "/parcels/:match*",
      "destination": "/parcels/:match*"
    },

    {
      "source": "/plotly/:match*",
      "destination": "/plotly/:match*"
    },

    {
      "source": "/test/:match*",
      "destination": "/test/:match*"
    },

    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },

    {
      "source": "/:match*",
      "destination": "/"
    }
  ]
}
